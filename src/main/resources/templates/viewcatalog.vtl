<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>Book Name</th>
      <th>Book Author</th>
      <th>Copies</th>
    </tr>
  </thead>
  <tbody>
  #foreach($book in $books)
    <tr>
      <td>$book.getId()</td>
      <td>$book.getTitle()</td>
      <td>$book.getAuthors()</td>
      <td>$book.getCopies()</td>
    </tr>
#end
  </tbody>
</table>
